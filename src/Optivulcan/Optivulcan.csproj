<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <PackageVersion>0.2.1</PackageVersion>
        <Title>Optivulcan</Title>
        <Authors>MRmlik12</Authors>
        <Description>A simple timetable parser for Optivum UONET+ written in C#</Description>
        <Copyright>MRmlik12</Copyright>
        <PackageProjectUrl>https://github.com/MRmlik12/Optivulcan</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/MRmlik12/Optivulcan/blob/main/LICENSE.md</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/MRmlik12/Optivulcan</RepositoryUrl>
        <PackageTags>Vulcan, School, Timetable, UONET+</PackageTags>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <LangVersion>10</LangVersion>
        <Nullable>enable</Nullable>
        <AssemblyVersion>0.2.1</AssemblyVersion>
        <FileVersion>0.2.1</FileVersion>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="AngleSharp" Version="0.16.1" />
        <PackageReference Include="AngleSharp.Io" Version="0.16.0" />
        <PackageReference Include="AngleSharp.XPath" Version="1.1.7" />
    </ItemGroup>

</Project>
